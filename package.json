{
  "name": "@ctsj/transform-package-es5",
  "version": "1.0.1",
  "description": "对package进行es5的转换",
  "bin": {
    "ctpe": "./bin/index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "generatorchangelog": "bd changelog/CHANGELOG.html",
    "init:husky": "npx husky add .husky/pre-commit \"npm run lint-staged\"",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\""
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/playerljc/CTSJ-TransformPackageES5.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/playerljc/CTSJ-TransformPackageES5/issues"
  },
  "homepage": "https://github.com/playerljc/CTSJ-TransformPackageES5#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "commander": "^9.5.0",
    "cross-env": "^7.0.3",
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "gulp": "^4.0.2",
    "rimraf": "^5.0.0",
    "core-js": "^3.30.0"
  },
  "devDependencies": {
    "@umijs/fabric": "^2.14.1",
    "eslint": "^8.38.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "stylelint": "^15.4.0"
  }
}
